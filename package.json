{
  "name": "ts-json-schema-generator",
  "version": "2.0.0",
  "description": "Generate JSON schema from your Typescript sources",
  "keywords": [
    "ts",
    "typescript",
    "json",
    "schema",
    "jsonschema"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vega/ts-json-schema-generator.git"
  },
  "license": "MIT",
  "author": {
    "name": "Alexander Evtushenko",
    "email": "aevtushenko@xiag.ch"
  },
  "contributors": [
    {
      "name": "Dominik Moritz",
      "email": "domoritz@gmail.com"
    },
    {
      "name": "MooYeol Prescott Lee",
      "email": "mooyoul@gmail.com"
    }
  ],
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "tjsg": "bin/tjsg.js",
    "tjsg-dev": "bin/tjsg-dev.js",
    "ts-json-schema-generator": "bin/tjsg.js"
  },
  "files": [
    "dist",
    "cjs",
    "src",
    "factory",
    "commands",
    "index.*",
    "ts-json-schema-generator.*"
  ],
  "scripts": {
    "build": "tsc",
    "postbuild": "oclif manifest && oclif readme && prettier --write README.md oclif.manifest.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.json",
    "debug": "tsx --inspect-brk bin/run.js",
    "format": "eslint --fix",
    "lint": "eslint",
    "prepublishOnly": "yarn build",
    "release": "yarn build && git diff && auto shipit",
    "run": "tsx bin/run.js",
    "test": "jest test/ --verbose",
    "test:coverage": "yarn jest test/ --collectCoverage=true",
    "test:fast": "cross-env FAST_TEST=1 jest test/ --verbose",
    "test:update": "cross-env UPDATE_SCHEMA=true yarn test:fast",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@oclif/core": "^3.26.6",
    "@oclif/plugin-autocomplete": "^3.0.18",
    "@oclif/plugin-help": "^6.0.22",
    "@oclif/plugin-not-found": "^3.1.10",
    "@oclif/plugin-warn-if-update-available": "^3.0.19",
    "@types/json-schema": "^7.0.15",
    "commander": "^12.0.0",
    "glob": "^10.3.12",
    "json5": "^2.2.3",
    "normalize-path": "^3.0.0",
    "safe-stable-stringify": "^2.4.3",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@auto-it/conventional-commits": "^11.1.6",
    "@auto-it/first-time-contributor": "^11.1.6",
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "@eslint/js": "^9.1.1",
    "@types/eslint": "^8.56.10",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/normalize-path": "^3.0.2",
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "auto": "^11.1.6",
    "chai": "^5.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "oclif": "^4.11.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.2",
    "typescript-eslint": "^7.7.1",
    "vega": "^5.28.0",
    "vega-lite": "^5.18.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "oclif": {
    "bin": "tjsg",
    "dirname": "ts-json-schema-generator",
    "binAliases": ["ts-json-schema-generator"],
    "topicSeparator": " ",
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ],
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "frequency": 30,
      "frequencyUnit": "seconds",
      "timeoutInDays": 7
    },
    "commands": "dist/commands"
  }
}
