{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "MyObject": {
            "type": "object",
            "properties": {
                "stringValue": {
                    "type": "string",
                    "title": "String field title",
                    "minLength": 10,
                    "format": "date-time",
                    "pattern": "/^\\d+$/"
                },
                "numberValue": {
                    "type": "integer",
                    "title": "Number field title",
                    "description": "This field is of integer type.\nInteger!",
                    "exclusiveMaximum": 10,
                    "multipleOf": 3
                },
                "privateString": {
                    "type": "string",
                    "title": "My private string",
                    "description": "Export field description",
                    "default": "private"
                },
                "exportString": {
                    "description": "Export field description",
                    "default": {"length": 10},
                    "anyOf": [
                        {
                            "$ref": "#/definitions/MyExportString"
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "numberArray": {
                    "$ref": "#/definitions/MyNonEmptyArray<number>",
                    "title": "Non empty array"
                },
                "number": {
                    "type": [
                        "number",
                        "null"
                    ]
                }
            },
            "required": [
                "stringValue",
                "numberValue",
                "exportString",
                "privateString",
                "numberArray",
                "number"
            ],
            "additionalProperties": false,
            "description": "Some description here",
            "title": "Some title here"
        },
        "MyExportString": {
            "type": "string",
            "title": "My export string"
        },
        "MyNonEmptyArray<number>": {
            "type": "array",
            "items": {
                "type": "number"
            },
            "minItems": 1
        }
    },
    "$ref": "#/definitions/MyObject"
}
